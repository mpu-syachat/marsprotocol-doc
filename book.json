{
    "title": "marsprotocol 帮助文档",
    "author": "changrui",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "plugins": [
        "-search",
        "-lunr",
        "-sharing",
        "-livereload",
        "search-plus",
        "hide-element",
        "sectionx",
        "multilingual-index"
    ], 
    "pluginsConfig": {
        "multilingual-index": { 
            "enableSuper": true, 
            "index":"index.html" 
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy www.marsprotocol.com 2023",
            "modify_label": "更新时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },

        "hide-element": {
            "elements": [".gitbook-link"]
        },

       "sectionx": {
          "tag": "b"
        }
    },

    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    }
}